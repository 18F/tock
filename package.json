{
  "private": true,
  "scripts": {
    "build-sass": "node-sass --source-map true tock/tock/static/sass -o tock/tock/static/css/dist",
    "watch-sass": "npm run build-sass -- --watch",
    "prestart": "npm install --quiet",
    "start": "npm run watch-sass",
    "predeploy": "npm install --silent && npm run build-sass"
  },
  "engines": {
    "node": "6.3.1"
  },
  "devDependencies": {
    "node-sass": "^3.9.3"
  }
}
