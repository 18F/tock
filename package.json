{
  "private": true,
  "scripts": {
    "build-css": "sass --source-map --load-path ./node_modules/uswds/src/stylesheets tock/tock/static/sass:tock/tock/static/css/dist",
    "watch-css": "npm run build-css -- --watch",
    "prestart": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=1 npm install --quiet",
    "start": "npm run build-css && npm run watch-css",
    "predeploy": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=1 npm install --silent && npm run build-css",
    "test": "JEST_PORT=8000 jest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "chosen-js": "^1.8.7",
    "jquery": "^3.6.0",
    "sass": "^1.45.0",
    "uswds": "^2.13.0"
  },
  "devDependencies": {
    "jest": "^27.0.6",
    "jest-puppeteer": "^3.9.1",
    "node-notifier": "^10.0.0",
    "puppeteer": "^1.20.0"
  }
}
