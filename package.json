{
  "private": true,
  "scripts": {
    "build-css": "sass --source-map --load-path ./node_modules/uswds/src/stylesheets tock/tock/static/sass:tock/tock/static/css/dist",
    "compile-uswds": "npx gulp init",
    "watch-css": "npm run build-css -- --watch",
    "prestart": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=1 npm install --quiet",
    "start": "npm run build-css && npm run watch-css",
    "predeploy": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=1 npm install --silent && npm run build-css",
    "test": "JEST_PORT=8000 jest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "chosen-js": "^1.8.7",
    "jquery": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "jest": "^27.0.6",
    "jest-puppeteer": "^3.9.1",
    "node-notifier": "^10.0.0",
    "postcss": "^8.4.5",
    "postcss-csso": "^5.0.1",
    "puppeteer": "^1.20.0",
    "sass": "^1.47.0",
    "uswds": "^2.13.0",
    "uswds-gulp": "github:uswds/uswds-gulp"
  }
}
